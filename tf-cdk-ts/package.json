{
  "name": "tf-cdk-ts",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "upgrade": "npm run upgrade:cdk && npm run upgrade:cli",
    "upgrade:cdk": "npm install --save-exact cdktf@latest",
    "upgrade:cli": "npm install --save-dev --save-exact cdktf-cli@latest",
    "clean": "tsc --build --clean",
    "prebuild": "npm run clean && npx ts-node ./scripts/prebuild.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postbuild": "npx cdktf synth",
    "postbuild:watch": "npx cdktf synth",
    "compile": "tsc --pretty",
    "postcompile": "npm run postbuild",
    "list": "npx cdktf list",
    "lint": "npx eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "0.8.15",
    "cdktf": "0.11.2",
    "constructs": "10.1.62"
  },
  "devDependencies": {
    "@azure/arm-compute": "19.1.0",
    "@azure/identity": "2.1.0",
    "@octokit/core": "3.6.0",
    "@types/jest": "27.5.2",
    "@types/js-yaml": "4.0.5",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "cdktf-cli": "0.11.2",
    "dotenv": "16.0.1",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "js-yaml": "4.1.0",
    "lint-staged": "13.0.3",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "unique-names-generator": "4.7.1"
  }
}
