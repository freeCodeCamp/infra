{
  "name": "tf-cdk-ts",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "build:watch": "tsc -w",
    "postbuild": "npx cdktf synth",
    "postbuild:watch": "npx cdktf synth",
    "compile": "tsc --pretty",
    "list": "npx cdktf list",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm run upgrade:cdk && npm run upgrade:cli",
    "upgrade:cdk": "npm install --save-exact cdktf@latest",
    "upgrade:cli": "npm install --save-dev --save-exact cdktf-cli@latest"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "0.5.53",
    "cdktf": "0.9.4",
    "constructs": "10.0.86"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "cdktf-cli": "0.9.4",
    "dotenv": "16.0.0",
    "jest": "27.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  }
}
