{
  "name": "tf-cdk-ts",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "upgrade": "npm run upgrade:cdk && npm run upgrade:cli",
    "upgrade:cdk": "npm install --save-exact cdktf@latest",
    "upgrade:cli": "npm install --save-dev --save-exact cdktf-cli@latest",
    "prebuild": "npx ts-node ./scripts/prebuild.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postbuild": "npx cdktf synth",
    "postbuild:watch": "npx cdktf synth",
    "build:dev": "tsc --pretty",
    "postbuild:dev": "npm run postbuild",
    "list": "npx cdktf list",
    "lint": "npx eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "^4.0.0",
    "cdktf": "^0.14.1",
    "constructs": "^10.1.169"
  },
  "devDependencies": {
    "@azure/arm-compute": "19.2.0",
    "@azure/identity": "3.1.2",
    "@octokit/core": "4.1.0",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.11.16",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cdktf-cli": "^0.14.1",
    "dotenv": "16.0.3",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "js-yaml": "4.1.0",
    "lint-staged": "13.1.0",
    "node-fetch": "3.3.0",
    "prettier": "^2.8.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "unique-names-generator": "4.7.1"
  }
}
