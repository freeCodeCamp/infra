{
  "name": "tf-cdk-ts",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "upgrade": "npm run upgrade:cdk && npm run upgrade:cli",
    "upgrade:cdk": "npm install --save-exact cdktf@latest",
    "upgrade:cli": "npm install --save-dev --save-exact cdktf-cli@latest",
    "prebuild": "npx ts-node ./scripts/prebuild.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postbuild": "npx cdktf synth",
    "postbuild:watch": "npx cdktf synth",
    "build:dev": "tsc --pretty",
    "postbuild:dev": "npm run postbuild",
    "list": "npx cdktf list",
    "lint": "npx eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "5.0.9",
    "cdktf": "0.15.5",
    "constructs": "10.1.274"
  },
  "devDependencies": {
    "@azure/arm-compute": "20.0.0",
    "@azure/identity": "3.1.3",
    "@octokit/core": "4.2.0",
    "@types/jest": "29.4.0",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.15.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "cdktf-cli": "0.15.5",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "js-yaml": "4.1.0",
    "lint-staged": "13.1.2",
    "node-fetch": "3.3.0",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "unique-names-generator": "4.7.1"
  }
}
