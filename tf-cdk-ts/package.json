{
  "name": "tf-cdk-ts",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "upgrade": "npm run upgrade:cdk && npm run upgrade:cli",
    "upgrade:cdk": "npm install --save-exact cdktf@latest",
    "upgrade:cli": "npm install --save-dev --save-exact cdktf-cli@latest",
    "prebuild": "npx ts-node ./scripts/prebuild.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postbuild": "npx cdktf synth",
    "postbuild:watch": "npx cdktf synth",
    "build:dev": "tsc --pretty",
    "postbuild:dev": "npm run postbuild",
    "list": "npx cdktf list",
    "lint": "npx eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "3.0.16",
    "cdktf": "0.13.3",
    "constructs": "10.1.151"
  },
  "devDependencies": {
    "@azure/arm-compute": "19.2.0",
    "@azure/identity": "3.0.0",
    "@octokit/core": "4.1.0",
    "@types/jest": "29.2.2",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "cdktf-cli": "0.13.3",
    "dotenv": "16.0.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.2.2",
    "js-yaml": "4.1.0",
    "lint-staged": "13.0.3",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "unique-names-generator": "4.7.1"
  }
}
