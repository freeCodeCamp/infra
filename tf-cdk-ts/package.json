{
  "name": "tf-cdk-ts",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "get": "cdktf get",
    "build": "tsc",
    "build:watch": "tsc -w",
    "postbuild": "npx cdktf synth",
    "postbuild:watch": "npx cdktf synth",
    "compile": "tsc --pretty",
    "list": "npx cdktf list",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm run upgrade:cdk && npm run upgrade:cli",
    "upgrade:cdk": "npm install --save-exact cdktf@latest",
    "upgrade:cli": "npm install --save-dev --save-exact cdktf-cli@latest"
  },
  "resolutions": {
    "colors": "1.4.0"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "0.4.125",
    "cdktf": "0.8.6",
    "constructs": "10.0.65"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "16.11.25",
    "cdktf-cli": "0.9.1",
    "dotenv": "10.0.0",
    "jest": "27.4.7",
    "npm-force-resolutions": "0.0.10",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
